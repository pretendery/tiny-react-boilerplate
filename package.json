  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --mode=development --config webpack.config.stdDev.js",
    "build": "webpack --progress --hide-modules --env.target=server --mode=production --config webpack.config.stdProd.js",
    "build:local": "webpack --progress --hide-modules --env.target=local --mode=production --config webpack.config.stdProd.js",
    "test": "jest --no-cache"
  },
  "browserslist": [
    "defaults"
  ],
  "jest": {
    "modulePathIgnorePatterns": [
      "node_modules"
    ]
  },
  "devDependencies": {
